{
  "name": "react-chat-popup",
  "version": "1.1.2",
  "description": "Chat web component for React apps",
  "main": "lib/index.js",
  "files": [
    "lib",
    "assets"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:JWebCoder/react-chat-popup.git"
  },
  "author": "JoÃ£o Moura <joao87moura@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "flow": "flow",
    "prepublish": "yarn run babel-prepublish && npm run flow-prepublish",
    "babel-prepublish": "babel src/ -d lib",
    "flow-prepublish": "flow-copy-source src lib"
  },
  "keywords": [
    "react",
    "chat",
    "popup",
    "javascript"
  ],
  "dependencies": {
    "lodash": "^4.17.5",
    "marked": "^0.3.6",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "7.5.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.68.0",
    "flow-cli": "^0.0.0-pre",
    "flow-copy-source": "^1.3.0",
    "husky": "^0.13.3",
    "jest": "^20.0.4",
    "prettier": "^1.1.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/props.js",
      "!**/state.js",
      "!**/node_modules/**",
      "!**/flow-typed/**",
      "!**/lib/**",
      "!**/example/**",
      "!**/coverage/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js"
    }
  },
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-native": "^0.55.1"
  }
}
